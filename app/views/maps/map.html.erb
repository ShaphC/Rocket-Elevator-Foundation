    
  <h1>Maps</h1>
  <div id="map"></div>
  <script>
    let map;
    function initMap() {
      map = new google.maps.Map(document.getElementById("map"), {
        center: { lat: -34.397, lng: 150.644 },
        zoom: 8,
      });
    }
  </script>

  <style>
    #map {
      height: 50%;
      width: 100%;
      align: center;
    }
    html,
    body {
      height: 100%;
      margin: 0;
      padding: 0;
    }
  </style>